#!/usr/bin/env bash
# A Bash script that kills the process 7-highlander
# Find the PID of the 7-highlander process
pid=$(pgrep -f "7-highlander")

# Check if the process is running
if [ -n "$pid" ]; then
    # Kill the process
    kill "$pid"
    echo "Process killed"
else
    echo "Process not found"
fi